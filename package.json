{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Final Project",
  "repository": "https://github.com/HackYourFuture-CPH/boilerplate-for-fp",
  "main": "index.js",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node ./src/server/bin/server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon ./src/server/bin/server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:clean": "cd ./src/server/ && knex migrate:rollback",
    "db:setup": "cd ./src/server/ && knex migrate:latest && knex seed:run",
    "db:reset": "npm run db:clean && npm run db:setup",
    "storybook": "cd ./src/client/ && start-storybook -p 3007",
    "test": "jest",
    "test:watch": "jest --watch",
    "code:check": "npm run code:lint && npm run code:format -- --check",
    "code:clean": "npm run code:lint -- --fix && npm run code:format -- --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier \"./src/**/**.{js,jsx,ts,tsx}\"",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "knex": "knex --cwd ./src/server"
  },
  "author": "Shanawaz Islam",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/react": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "app-root-path": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.1",
    "chalk": "^5.0.0",
    "circular-json": "^0.5.9",
    "clean-webpack-plugin": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^7.1.0",
    "express": "^4.17.2",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jest-css-modules-transform": "^4.3.0",
    "knex": "> 0.8 < 0.96",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "mssql": "^8.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node-sass": "^7.0.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "sass-loader": "^12.4.0",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "winston": "^3.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint",
    "*.{js,css,md}": "prettier --write",
    "*.css": "stylelint --fix"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "concurrently": "^7.0.0",
    "express-form-data": "^2.0.17",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "mock-knex": "^0.4.10",
    "nodemon": "^2.0.15",
    "poetic": "^1.7.0",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "webpack-dev-server": "^4.7.4"
  }
}
